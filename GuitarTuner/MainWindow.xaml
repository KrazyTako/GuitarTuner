<Window x:Class="GuitarTuner.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GuitarTuner"
        mc:Ignorable="d"
        Title="MainWindow" Height="900" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="117*"/>
            <ColumnDefinition Width="312*"/>
            <ColumnDefinition Width="371*"/>
        </Grid.ColumnDefinitions>
        <WpfPlot x:Name="PlotGraph" HorizontalAlignment="Left" VerticalAlignment="Top" Height="405" Width="586" Grid.ColumnSpan="2" Margin="22,54,0,0" Grid.Column="1"/>
        <ProgressBar x:Name="ProgressBar" HorizontalAlignment="Left" Height="10" Margin="265,39,0,0" VerticalAlignment="Top" Width="100" Grid.ColumnSpan="2" Grid.Column="1"/>
        <WpfPlot Name="FrequencyGraph" HorizontalAlignment="Left" Margin="22,464,0,0" VerticalAlignment="Top" Grid.ColumnSpan="2" Grid.Column="1" Height="388" Width="586"/>

    </Grid>
</Window>
